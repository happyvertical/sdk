{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "./src",
    "tsBuildInfoFile": "./dist/.tsbuildinfo-browser",
    "lib": ["ES2023", "DOM", "DOM.Iterable"],
    "types": [],
    "composite": true
  },
  "include": [
    "src/shared/**/*",
    "src/browser/**/*",
    "src/index.ts",
    "src/browser.ts"
  ],
  "exclude": [
    "src/node/**/*",
    "**/*.test.ts",
    "**/*.spec.ts"
  ]
}