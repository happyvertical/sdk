{
  "name": "@have/sql",
  "version": "0.0.50",
  "description": "Database interface with support for SQLite and PostgreSQL",
  "type": "module",
  "main": "dist/node.js",
  "browser": "dist/browser.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "browser": "./dist/browser.js",
      "node": "./dist/node.js",
      "default": "./dist/node.js"
    },
    "./browser": "./dist/browser.js",
    "./node": "./dist/node.js"
  },
  "scripts": {
    "test": "bun test",
    "test:watch": "bun test --watch",
    "build": "bun run build:browser && bun run build:node",
    "build:browser": "tsc -p tsconfig.browser.json",
    "build:node": "tsc -p tsconfig.node.json",
    "build:watch": "bun run build:browser --watch & bun run build:node --watch",
    "clean": "rm -rf dist *.tsbuildinfo",
    "dev": "bun run build:watch & bun run test:watch"
  },
  "keywords": [],
  "author": "willgriffin@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@have/utils": "workspace:*",
    "@libsql/client": "^0.14.0",
    "sqlite-vss": "^0.1.2",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.0",
    "@types/pg": "^8.11.10",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
