{
  "name": "@have/pdf",
  "version": "0.0.50",
  "description": "Modern PDF processing utilities with text extraction and OCR support using unpdf and @gutenye/ocr-node",
  "type": "module",
  "main": "dist/node.js",
  "browser": "dist/browser.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "browser": "./dist/browser.js",
      "node": "./dist/node.js",
      "default": "./dist/node.js"
    },
    "./browser": "./dist/browser.js",
    "./node": "./dist/node.js"
  },
  "scripts": {
    "test": "bun test --timeout 60000",
    "test:watch": "bun test --watch --timeout 60000",
    "build": "bun run build:node && bun run build:browser",
    "build:node": "tsc -p tsconfig.node.json",
    "build:browser": "tsc -p tsconfig.browser.json",
    "build:watch": "bun run build:node --watch & bun run build:browser --watch",
    "clean": "rm -rf dist",
    "clean:all": "rm -rf dist *.tsbuildinfo node_modules",
    "prebuild": "bun run clean",
    "dev": "bun run build:watch & bun run test:watch"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "@have/ocr": "workspace:*",
    "unpdf": "^1.0.6"
  },
  "devDependencies": {
    "@types/node": "^22.13.0"
  }
}
