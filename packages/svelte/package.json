{
  "name": "@have/svelte",
  "version": "0.0.44",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && pnpm run prepack",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "prepack": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./styles.css": "./dist/styles/index.css",
    "./Card.svelte": {
      "types": "./dist/components/cards/Card.svelte.d.ts",
      "svelte": "./dist/components/cards/Card.svelte",
      "default": "./dist/components/cards/Card.svelte"
    },
    "./Article.svelte": {
      "types": "./dist/components/articles/Article.svelte.d.ts",
      "svelte": "./dist/components/articles/Article.svelte",
      "default": "./dist/components/articles/Article.svelte"
    },
    "./ArticleList.svelte": {
      "types": "./dist/components/articles/ArticleList.svelte.d.ts",
      "svelte": "./dist/components/articles/ArticleList.svelte",
      "default": "./dist/components/articles/ArticleList.svelte"
    },
    "./ArticleListItem.svelte": {
      "types": "./dist/components/articles/ArticleListItem.svelte.d.ts",
      "svelte": "./dist/components/articles/ArticleListItem.svelte",
      "default": "./dist/components/articles/ArticleListItem.svelte"
    }
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@storybook/addon-essentials": "^8.5.5",
    "@storybook/addon-interactions": "^8.5.5",
    "@storybook/addon-svelte-csf": "^5.0.0-next.23",
    "@storybook/blocks": "^8.5.5",
    "@storybook/svelte": "^8.5.5",
    "@storybook/sveltekit": "^8.5.5",
    "@storybook/test": "^8.5.5",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@tailwindcss/vite": "^4.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^2.46.1",
    "globals": "^15.14.0",
    "postcss": "^8.5.2",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "publint": "^0.3.2",
    "storybook": "^8.5.5",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^4.0.6",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.0.0"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.0.1"
  }
}
