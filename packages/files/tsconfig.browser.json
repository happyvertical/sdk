{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "./src",
    "tsBuildInfoFile": "./dist/.tsbuildinfo-browser",
    "lib": ["ES2023", "DOM", "DOM.Iterable", "WebWorker"],
    "types": [],
    "composite": true
  },
  "include": [
    "src/shared/types.ts",
    "src/shared/base.ts",
    "src/shared/factory.ts",
    "src/browser/**/*",
    "src/browser.ts",
    "src/index.ts"
  ],
  "exclude": [
    "src/node/**/*",
    "src/node.ts",
    "src/providers/**/*",
    "src/shared/s3.ts",
    "src/shared/webdav.ts",
    "src/shared/gdrive.ts",
    "src/legacy.ts",
    "src/factory.ts",
    "src/filesystem*.ts",
    "src/fetch.ts",
    "**/*.test.ts",
    "**/*.spec.ts"
  ]
}